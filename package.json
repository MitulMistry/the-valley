{
  "name": "the-valley",
  "version": "1.0.0",
  "description": "An HTML5 text adventure system using Phaser",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "webpack",
    "deploy": "webpack --config webpack.production.config.js",
    "cordova": "webpack --config webpack.cordova.config.js",
    "test": "eslint './src/**/*.js'",
    "test-fix": "eslint './src/**/*.js' --fix",
    "postinstall": "webpack --config webpack.production.config.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MitulMistry/the-valley.git"
  },
  "keywords": [
    "text adventure",
    "game",
    "phaser"
  ],
  "author": "Mitul Mistry",
  "bugs": {
    "url": "https://github.com/MitulMistry/the-valley/issues"
  },
  "homepage": "https://github.com/MitulMistry/the-valley#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cordova": "^8.1.1",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "expose-loader": "^0.7.5",
    "html-webpack-plugin": "^2.30.1",
    "webpack": "^3.12.0"
  },
  "dependencies": {
    "cordova-android": "^7.1.1",
    "cordova-browser": "^5.0.4",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-whitelist": "^1.3.3",
    "express": "^4.16.3",
    "phaser-ce": "^2.11.0",
    "webfontloader": "^1.6.28"
  },
  "engines": {
    "node": "8.9.1"
  },
  "cordova": {
    "platforms": [
      "android",
      "browser",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {}
    }
  }
}
