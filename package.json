{
  "name": "the-valley",
  "version": "1.0.0",
  "description": "An HTML5 text adventure system using Phaser",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "dev": "webpack",
    "deploy": "webpack --config webpack.production.config.js",
    "cordova": "webpack --config webpack.cordova.config.js",
    "test": "eslint './src/**/*.js'",
    "test-fix": "eslint './src/**/*.js' --fix",
    "heroku-prebuild": "npm install --dev",
    "postinstall": "webpack --config webpack.production.config.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MitulMistry/the-valley.git"
  },
  "keywords": [
    "text adventure",
    "game",
    "phaser"
  ],
  "author": "Mitul Mistry",
  "bugs": {
    "url": "https://github.com/MitulMistry/the-valley/issues"
  },
  "homepage": "https://github.com/MitulMistry/the-valley#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cordova": "^9.0.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "expose-loader": "^0.7.5",
    "html-webpack-plugin": "^3.2.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "cordova-android": "^8.1.0",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "express": "^4.17.1",
    "phaser-ce": "^2.14.0",
    "webfontloader": "^1.6.28"
  },
  "engines": {
    "node": "10.14.1"
  },
  "cordova": {
    "platforms": [
      "android",
      "browser",
      "ios"
    ],
    "plugins": {
      "cordova-plugin-whitelist": {}
    }
  }
}
